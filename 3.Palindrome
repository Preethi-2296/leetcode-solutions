class Solution(object):
    def isPalindrome(self, x):
        num= x
        reversed= 0

        if x < 0:
            x = -x  

        while x > 0:
            digit = x % 10
            reversed= reversed * 10 + digit
            x = x // 10  

        if reversed== num :
            return True
        else:
            return False
